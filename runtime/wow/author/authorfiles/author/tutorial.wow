<!DOCTYPE conceptList SYSTEM "../generatelist4.dtd">

<conceptList>
<name>tutorial</name>

<concept>
 <name>tutorial.welcome</name>
 <description>The main welcome page (reached through index)</description>
 <resource>file:/tutorial/xml/welcome.xhtml</resource> 
 <stable></stable>
 <concepttype>page</concepttype>
 <title>welcome</title>
 <hierarchy>
   <firstchild></firstchild>
   <nextsib>tutorial.installation</nextsib>
   <parent>tutorial.tutorial</parent>
 </hierarchy>
  <attribute name="access" type="bool" isPersistent="false" isSystem="true" isChangeable="false">
   <description>triggered by page access</description>
   <default>false</default>
    <generateListItem isPropagating="false" >
      <requirement>true</requirement>
      <trueActions>
       <action>
        <conceptName>tutorial.testtutorial</conceptName>
        <attributeName>hierarchy</attributeName>
        <expression>false</expression>
       </action>
      </trueActions>
    </generateListItem>
    <generateListItem isPropagating="true" >
      <requirement>! tutorial.welcome.suitability &amp;&amp; tutorial.welcome.knowledge &lt; 35</requirement>
      <trueActions>
       <action>
        <conceptName>tutorial.welcome</conceptName>
        <attributeName>knowledge</attributeName>
        <expression>35</expression>
       </action>
      </trueActions>
    </generateListItem>
    <generateListItem isPropagating="true" >
      <requirement>tutorial.welcome.suitability</requirement>
      <trueActions>
       <action>
        <conceptName>tutorial.welcome</conceptName>
        <attributeName>knowledge</attributeName>
        <expression>100</expression>
       </action>
      </trueActions>
    </generateListItem>
    <generateListItem isPropagating="true" >
      <requirement>tutorial.welcome.suitability</requirement>
      <trueActions>
       <action>
        <conceptName>tutorial.welcome</conceptName>
        <attributeName>visited</attributeName>
        <expression>tutorial.welcome.visited + 1</expression>
       </action>
      </trueActions>
    </generateListItem>
    <generateListItem isPropagating="true" >
      <requirement>tutorial.welcome.visited&gt;1</requirement>
      <trueActions>
       <action>
        <conceptName>tutorial.welcome</conceptName>
        <attributeName>showability</attributeName>
        <expression>1</expression>
       </action>
      </trueActions>
    </generateListItem>
    <generateListItem isPropagating="true" >
      <requirement>!((tutorial.welcome.visited&gt;1))</requirement>
      <trueActions>
       <action>
        <conceptName>tutorial.welcome</conceptName>
        <attributeName>showability</attributeName>
        <expression>0</expression>
       </action>
      </trueActions>
    </generateListItem>
  </attribute>
  <attribute name="knowledge" type="int" isPersistent="true" isSystem="false" isChangeable="true">
   <description>knowledge about this concept</description>
   <default>0</default>
    <generateListItem isPropagating="true" >
      <requirement>_tutorial.welcome.knowledge &gt; 0</requirement>
      <trueActions>
       <action>
        <conceptName>tutorial.tutorial</conceptName>
        <attributeName>knowledge</attributeName>
        <expression>tutorial.tutorial.knowledge + (0.1111111111111111 * _tutorial.welcome.knowledge)</expression>
       </action>
      </trueActions>
    </generateListItem>
  </attribute>
  <attribute name="visited" type="int" isPersistent="true" isSystem="true" isChangeable="false">
   <description>has this page been visited?</description>
   <default>0</default>
  </attribute>
  <attribute name="suitability" type="bool" isPersistent="false" isSystem="false" isChangeable="false">
   <description>the suitability of this page</description>
   <default>true</default>
  </attribute>
  <attribute name="interest" type="int" isPersistent="true" isSystem="false" isChangeable="true">
   <description>is the user interested in this concept</description>
   <default>0</default>
  </attribute>
  <attribute name="showability" type="int" isPersistent="true" isSystem="false" isChangeable="false">
   <description></description>
   <default>0</default>
   <casegroup>
     <defaultfragment>file:/tutorial/xml/readme.xhtml</defaultfragment>
       <casevalue>
         <value>1</value>
         <returnfragment>file:/tutorial/xml/welcome.xhtml</returnfragment>
       </casevalue>
     </casegroup>
  </attribute>
 </concept>

